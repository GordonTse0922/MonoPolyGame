package test;
import org.junit.jupiter.api.*;
import static org.junit.jupiter.api.Assertions.*;

import Model.ChanceSquare;


public class ChanceSquareTest {
	ChanceSquare chanceSquare = new ChanceSquare("TestChanceSqaure", 0);
	
    @Test
    public void chanceSquareTestSetGetName(){
    	chanceSquare.setName("TestChance");
    	String result = chanceSquare.getName(); 
    	assertEquals("TestChance", result);
    }
}
